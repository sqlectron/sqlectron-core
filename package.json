{
  "name": "@armarti/unified-dataloader-core",
  "version": "0.0.5",
  "license": "GPL-3.0-only",
  "description": "@armarti/unified-dataloader-core",
  "author": "Armarti Industries",
  "contributors": [
    "David Charles Morse <dcmorse@armarti.com>",
    "The SQLECTRON Team"
  ],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/armarti/unified-dataloader-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "rimraf lib && babel -d lib/ src/",
    "watch": "rimraf lib && babel --source-maps=both --watch -d lib/ src/",
    "prepublish": "yarn run compile",
    "lint": "eslint src/ spec/",
    "test:mysql": "DB_CLIENTS=mysql yarn run test",
    "test:sqlite": "DB_CLIENTS=sqlite yarn run test",
    "test:sqlserver": "DB_CLIENTS=sqlserver yarn run test",
    "test:postgresql": "DB_CLIENTS=postgresql yarn run test",
    "test:cassandra": "DB_CLIENTS=cassandra yarn run test",
    "test": "yarn run lint && mocha --timeout 40000 --compilers js:@babel/register --reporter spec ./spec/**/*.spec.js",
    "watch-test": "yarn run lint && mocha --compilers js:babel/register --watch --reporter spec ./spec/**/*.spec.js"
  },
  "dependencies": {
    "cassandra-driver": "^3.5.0",
    "debug": "^4.0.1",
    "env-paths": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mssql": "^4.2.1",
    "mysql": "^2.16.0",
    "pg": "git+https://github.com/maxcnunes/node-postgres.git#multiple-statements-v6.1.0",
    "portfinder": "^1.0.17",
    "rimraf": "^2.6.2",
    "sql-query-identifier": "^1.1.0",
    "sqlite3": "^4.0.1",
    "ssh2": "^0.6.1",
    "uuid": "^3.3.2",
    "valida": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">=6 <10.2",
    "yarn": ">=1.0"
  }
}
